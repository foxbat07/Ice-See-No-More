{
  "name": "icecantsee",
  "version": "1.0.0",
  "description": "Load Antarctica Terrains",
  "main": "server.js",
  "scripts": {
    "build": "npm install",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "rebuild": "rm -rf node_modules && npm install",
    "open": "concurrently \"http-server -a localhost -p 3033\" \"sleepms 1000 && open http://localhost:3033\"",
    "launch": " npm run dev && sleepms 10000 && npm run open"
  },
  "keywords": [
    "creative",
    "code"
  ],
  "author": "Mohit Hingorani",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.17.3",
    "helmet": "^3.22.0",
    "nodemon": "^2.0.3"
  },
  "engines": {
    "node": "13.13.0"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "http-server": "^0.12.3",
    "sleep-ms": "^2.0.1"
  }
}
